function setUp() {
	M2S=./mysql2sqlite
	testDir=unit_tests
}

function tearDown() {
	rm -f actual.out expected.out
}

function testICanTestOne()
{
	# Given
	cat ${testDir}/test_one.sql.out >expected.out
	
	# When
	${M2S} ${testDir}/test_one.sql >actual.out

	# Then
	# assertExitSuccessfully
	
	# assertFileNotEmpty actual.out
	
	assertNoDiff expected.out actual.out
}

